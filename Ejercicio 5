class Profesor:
    """Superclase que representa un profesor gen√©rico."""
    def imprimir(self):
        print("Es un profesor.")

class ProfesorTitular(Profesor):
    """Subclase que representa un profesor titular."""
    def imprimir(self):
        print("Es un profesor titular.")

def main():
    # Polimorfismo: la referencia es de tipo Profesor
    profesor1: Profesor = ProfesorTitular()
    profesor1.imprimir()

if __name__ == "__main__":
    main()
